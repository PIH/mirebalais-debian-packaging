#!/bin/sh

set -e

case "$1" in
    configure)
        chown -R tomcat6:tomcat6 /home/ffreire/.OpenMRS;
        service tomcat6 start
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
        service tomcat6 start
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0;
